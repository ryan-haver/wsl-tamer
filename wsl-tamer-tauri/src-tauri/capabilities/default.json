{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window and tray",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "core:window:allow-show",
    "core:window:allow-hide",
    "core:window:allow-set-focus",
    "core:window:allow-close",
    "core:tray:default",
    "core:menu:default",
    "opener:default",
    "store:default",
    "autostart:default",
    "dialog:default",
    "shell:default",
    "shell:allow-execute",
    "shell:allow-open",
    "fs:default",
    "fs:read-all",
    "fs:write-all",
    "process:default"
  ]
}
